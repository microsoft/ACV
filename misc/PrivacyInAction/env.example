# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_API_VERSION=2024-09-01-preview
AZURE_OPENAI_DEPLOYMENT=o3-20250416

# Notion API Configuration
NOTION_API_KEY=your_notion_api_key_here
NOTION_PARENT_ID=your_notion_parent_page_id_here

# Gmail Token Path (optional, defaults to token.json)
GMAIL_TOKEN_PATH=token.json

# Azure AD Authentication (if using service principal)
# AZURE_CLIENT_ID=your_client_id
# AZURE_CLIENT_SECRET=your_client_secret
# AZURE_TENANT_ID=your_tenant_id

